{
  "export": "doctrine2-annotation",
  "zip": false,
  "dir": "src/CoreBundle/EntityExport",
  "params": {
    "backupExistingFile": true,
    "skipPluralNameChecking": false,
    "enhanceManyToManyDetection": false,
    "bundleNamespace": "CoreBundle",
    "entityNamespace": "Entity",
    "repositoryNamespace": "CoreBundle\\Entity\\Repository",
    "useAnnotationPrefix": "ORM\\",
    "useAutomaticRepository": true,
    "indentation": 4,
    "filename": "%entity%.%extension%",
    "quoteIdentifier": false,
    "propertyNameCodingStyle": "lowercamelcase",
    "tableNameCodingStyle": "underscore",
    "columnNameCodingStyle": "underscore"
  }
}